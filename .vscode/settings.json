{
    "mcp": {
        "inputs": [],
        "servers": {
            "Context7": {
                "type": "stdio",
                "command": "npx",
                "args": [
                    "-y",
                    "@upstash/context7-mcp@latest"
                ]
            },
            "shrimp-task-manager": {
                "command": "npx",
                "args": [
                    "-y",
                    "mcp-shrimp-task-manager"
                ],
                "env": {
                    "DATA_DIR": "/home/marjan/Work/PROJECTS/ExpenseTracker/tasks",
                    "TEMPLATES_USE": "en",
                    "ENABLE_GUI": "true"
                }
            },
            "sequential-thinking": {
                "type": "stdio", // Added for consistency; optional if not required by your extension
                "command": "npx",
                "args": [
                    "-y",
                    "@modelcontextprotocol/server-sequential-thinking"
                ]
            },
            "github-remote": {
                "type": "http",
                "url": "https://api.githubcopilot.com/mcp/"
            },
            "puppeteer": {
                "command": "npx",
                "args": [
                    "-y",
                    "@modelcontextprotocol/server-puppeteer"
                ]
            }
        }
    }
}